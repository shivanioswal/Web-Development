<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <servlet>
  	<servlet-name> logindemo</servlet-name>
  	<servlet-class> com.practice.Login</servlet-class>
  </servlet>
  <servlet-mapping> 
  	<servlet-name> logindemo </servlet-name>
  	<url-pattern> /login </url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name> profiledemo</servlet-name>
  	<servlet-class> com.practice.profile</servlet-class>
  </servlet>
  <servlet-mapping> 
  	<servlet-name> profiledemo </servlet-name>
  	<url-pattern> /profile </url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name> logoutdemo</servlet-name>
  	<servlet-class> com.practice.Logout</servlet-class>
  </servlet>
  <servlet-mapping> 
  	<servlet-name> logoutdemo </servlet-name>
  	<url-pattern> /Logout </url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
</web-app>